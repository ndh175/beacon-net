{
    "identityPoolName": "beaconnet",
    "allowUnauthenticatedIdentities": false,
    "thirdPartyAuth": false,
    "lambdaLogPolicy": "beaconnet_lambda_log_policy",
    "openIdLambdaRoleName": "beaconnet_openid_lambda_role",
    "openIdRolePolicy": "beaconnet_openid_pass_role_policy",
    "openIdLambdaIAMPolicy": "beaconnet_openid_lambda_iam_policy",
    "openIdLogPolicy": "beaconnet_openid_lambda_log_policy",
    "userPoolName": "beaconnet",
    "autoVerifiedAttributes": [
        "email"
    ],
    "mfaConfiguration": "OFF",
    "mfaTypes": [
        "SMS Text Message"
    ],
    "roleName": "beaconnet_sns-role",
    "roleExternalId": "beaconnet_role_external_id",
    "policyName": "beaconnet-sns-policy",
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your verification code",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": false,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [
        "Requires Lowercase",
        "Requires Uppercase",
        "Requires Numbers",
        "Requires Symbols"
    ],
    "requiredAttributes": [
        "email"
    ],
    "userpoolClientName": "beaconnet_app_client",
    "userpoolClientGenerateSecret": true,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientReadAttributes": [
        "email"
    ],
    "mfaLambdaRole": "beaconnet_totp_lambda_role",
    "mfaLambdaLogPolicy": "beaconnet_totp_lambda_log_policy",
    "mfaPassRolePolicy": "beaconnet_totp_pass_role_policy",
    "mfaLambdaIAMPolicy": "beaconnet_totp_lambda_iam_policy",
    "userpoolClientLambdaRole": "beaconnet_userpoolclient_lambda_role",
    "userpoolClientLogPolicy": "beaconnet_userpoolclient_lambda_log_policy",
    "userpoolClientLambdaPolicy": "beaconnet_userpoolclient_lambda_iam_policy",
    "userpoolClientSetAttributes": false,
    "useDefault": "manual",
    "authSelections": "identityPoolAndUserPool",
    "resourceName": "beaconnet",
    "authRoleName": {
        "Ref": "AuthRoleName"
    },
    "unauthRoleName": {
        "Ref": "UnauthRoleName"
    },
    "authRoleArn": {
        "Fn::GetAtt": [
            "AuthRole",
            "Arn"
        ]
    },
    "unauthRoleArn": {
        "Fn::GetAtt": [
            "UnauthRole",
            "Arn"
        ]
    }
}